<SimpleHbase>
	<HBaseTableSchema tableName="area" defaultFamily="areaFamily">
		<HBaseColumnSchema qualifier="id" typeName="int" />
		<HBaseColumnSchema qualifier="name" typeName="string" />
		<HBaseColumnSchema qualifier="code" typeName="string" />
		<HBaseColumnSchema qualifier="cityCode" typeName="string" />
		<HBaseColumnSchema qualifier="createTime" typeName="java.util.Calendar" handler="com.alipay.simplehbase.type.handler.CalendarHandler"/>
	</HBaseTableSchema>	
	<MappingType className="com.qingting.customer.model.Area" defaultFamily="areaFamily">
		<field name="id"/>
		<field name="name"/>
		<field name="code" />
		<field name="cityCode" />
		<field name="createTime"/>
	</MappingType>
</SimpleHbase>   



