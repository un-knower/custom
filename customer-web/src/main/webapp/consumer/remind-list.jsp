							}						
						}
				});
				function drawWarnList(data){
					var warnDiv = '';
					for (var i in data){
						data[i].leak == true?data[i].leak='是':data[i].leak='否';
					 	warnDiv += '<li class="xy-layout-bar xy-border-box" deviceCode="'+data[i].equipCode+'">'+
										'<a class="weui-cell xy-fc-black" href="#">'+
											'<div>'+
												'<div class="weui-cell__bd xy-clearfix">'+
													'<div class="xy-tac">'+
														'<div class="xy-dib">'+
															'<p class="mini-lovely"><img src="${_staticPath}'+data[i].path+'" /></p>'+
															'<p class="xy-pad-t3 xy-fs14">'+data[i].equipName+'</p>'+
														'</div>'+
													'</div>'+
												'</div>'+
												'<div class="xy-dataListBox xy-dataListNoLine">'+
													'<div class="xy-dataList">'+
														'<div class="xy-dataItem">'+
															'<div class="xy-dataItemName">原水TDS值</div>'+
															'<div class="xy-dataItemValue">'+data[i].rawTds+'</div>'+
														'</div>'+
													'</div>'+
													'<div class="xy-dataList">'+
														'<div class="xy-dataItem">'+
															'<div class="xy-dataItemName">净水TDS值</div>'+
															'<div class="xy-dataItemValue">'+data[i].purTds+'</div>'+
														'</div>'+
													'</div>'+
													'<div class="xy-dataList">'+
														'<div class="xy-dataItem">'+
															'<div class="xy-dataItemName">流量</div>'+
															'<div class="xy-dataItemValue">'+data[i].flow+'</div>'+
														'</div>'+
													'</div>'+
													'<div class="xy-dataList">'+
														'<div class="xy-dataItem">'+
															'<div class="xy-dataItemName">湿度</div>'+
